{
  "name": "discuss",
  "description": "Piman Discuss, La solution de communication privee, dediee, sur mesure et chiffree",
  "version": "5.9.8",
  "main": "./main.js",
  "author": "PRIVATE DISCUSS <contact@piman-group.fr>",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --publish never",
    "deploy": "electron-builder --publish always",
    "dist:linux": "electron-builder --linux"
  },
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "electron-context-menu": "^0.9.1",
    "electron-dl": "^1.12.0",
    "electron-progressbar": "^1.1.0",
    "electron-updater": "^4.3.5",
    "fs": "0.0.1-security",
    "i18next": "^19.7.0",
    "i18next-node-fs-backend": "^2.1.3",
    "image-data-uri": "^2.0.0",
    "offline-js": "^0.7.19",
    "v8-compile-cache": "^2.3.0",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "electron": "^36.3.1",
    "electron-builder": "^23.0.2",
    "electron-installer-dmg": "^3.0.0",
    "electron-notarize": "^1.0.0",
    "electron-packager": "^14.1.0",
    "electron-winstaller": "^4.0.0"
  },
  "build": {
    "appId": "com.piman-discuss.piman",
    "mac": {
      "category": "productivity",
      "gatekeeperAssess": false,
      "hardenedRuntime": true,
      "entitlements": "./build/entitlements.mac.plist",
      "entitlementsInherit": "./build/entitlements.mac.inherit.plist"
    },
    "linux": {
      "icon": "build/icons/Icon-512x512.png",
      "category": "productivity",
      "target": [
        "AppImage"
      ]
    },
    "win": {
      "icon": "./build/icon.ico",
      "target": "msi"
    },
    "directories": {
      "output": "app",
      "buildResources": "build"
    },
    "productName": "Piman Discuss",
    "protocols": [
      {
        "name": "Piman discuss",
        "schemes": [
          "piman-discuss"
        ]
      }
    ]
  },
  "deploy": "electron-builder build --mac --win --publish always",
  "repository": {
    "type": "git",
    "url": "https://github.com/kawtarnouara/discuss-auto-update.git"
  },
  "mac": {
    "title": "Piman Discuss",
    "hardenedRuntime": true,
    "gatekeeperAssess": false,
    "entitlements": "build/entitlements.mac.plist",
    "entitlementsInherit": "build/entitlements.mac.inherit.plist"
  },
  "mas": {
    "entitlements": "build/entitlements.mac.plist",
    "entitlementsInherit": "build/entitlements.mac.inherit.plist"
  }
}
